<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <TextField fx:id="userName" layoutX="212.0" layoutY="123.0" />
      <Label layoutX="439.0" layoutY="127.0" text="User Name">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label layoutX="439.0" layoutY="179.0" text="Password">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Button fx:id="loginButton" layoutX="269.0" layoutY="303.0" mnemonicParsing="false" onAction="#attemptLogin" text="Login">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Button>
      <RadioButton fx:id="member" layoutX="212.0" layoutY="241.0" mnemonicParsing="false" text="Member">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></RadioButton>
      <RadioButton fx:id="admin" layoutX="341.0" layoutY="241.0" mnemonicParsing="false" text="Admin">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></RadioButton>
      <PasswordField fx:id="password" layoutX="212.0" layoutY="175.0" />
      <Label layoutX="109.0" layoutY="40.0" prefHeight="42.0" prefWidth="463.0" text="Welcome to AdminMember App" textFill="#ee3a3a">
         <font>
            <Font name="Comic Sans MS Bold" size="30.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
